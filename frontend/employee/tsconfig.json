{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2023",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "allowJs": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@/services/*": [
        "./services/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/libs/*": [
        "./libs/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ],
      "@/contexts/*": [
        "./contexts/*"
      ]
    },
    "types": [
      "node"
    ],
    "incremental": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "src",
    "next.config.*",
    "tailwind.config.*",
    "postcss.config.*",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules", ".next"]

}
